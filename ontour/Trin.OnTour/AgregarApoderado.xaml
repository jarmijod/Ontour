        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Trin.OnTour"
        mc:Ignorable="d"
        Title="AgregarApoderado" Height="471" Width="440">
    <Grid>
        <Button x:Name="BTN_ingresarA" Content="Ingresar" HorizontalAlignment="Left" Margin="292,381,0,0" VerticalAlignment="Top" Width="90" FontSize="16" Height="35" Click="BTN_ingresarA_Click"/>
        <Label Content="Nombre Apoderado" HorizontalAlignment="Left" Margin="45,68,0,0" VerticalAlignment="Top" FontSize="14"/>
